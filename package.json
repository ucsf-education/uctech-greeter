{
  "name": "uctech-greeter",
  "version": "2.0.0",
  "description": "A bot that welcomes new users to the UCTech team on Slack.",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node ./dist/app.js",
    "lint": "eslint --ignore-pattern=\"dist\"",
    "format": "prettier . --write",
    "test": "npm run build && node --import tsx --test"
  },
  "keywords": [
    "application",
    "bolt",
    "slack",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ucsf-education/uctech-greeter"
  },
  "author": "Stefan Topfstedt",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.0.0",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "@slack/bolt": "^4.6.0",
    "@tsconfig/node24": "^24.0.4",
    "dotenv": "^17.2.3",
    "typescript": "^5.9.3"
  }
